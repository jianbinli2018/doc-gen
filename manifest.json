            "16": "./resource/img/doc-gen-16.png",
            "32": "./resource/img/doc-gen-32.png",
            "64": "./resource/img/doc-gen-64.png",
            "96": "./resource/img/doc-gen-96.png",
            "128": "./resource/img/doc-gen-128.png"
        }
    },
    "icons": {
        "16": "./resource/img/doc-gen-16.png",
        "32": "./resource/img/doc-gen-32.png",
        "64": "./resource/img/doc-gen-64.png",
        "96": "./resource/img/doc-gen-96.png",
        "128": "./resource/img/doc-gen-128.png"
    },
    "manifest_version": 2
}